<p id="notice"><%= notice %></p>

<h1>Listing Simplefroms</h1>

<table class="table hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Year</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @simplefroms.each do |simplefrom| %>
      <tr>
        <td><%= simplefrom.name %></td>
        <td><%= simplefrom.email %></td>
        <td><%= simplefrom.year %></td>
        <td><%= simplefrom.role_id %></td>
        <td><%= link_to 'Show', simplefrom %></td>
        <td><%= link_to 'Edit', edit_simplefrom_path(simplefrom) %></td>
        <td><%= link_to 'Destroy', simplefrom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Simplefrom', new_simplefrom_path %>
